[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = tab
insert_final_newline = false
max_line_length = 140
tab_width = 2
trim_trailing_whitespace = true

[{*.cjs,*.js,*.ts}]
ij_javascript_use_double_quotes = true


[{*.htm,*.html,*.sht,*.shtm,*.shtml}]
ij_html_add_new_line_before_tags = true
ij_html_indent_size = 2
ij_html_tab_width = 2
